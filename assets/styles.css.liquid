.rimage-wrapper {
  position: relative;
  transition: background 500ms;
}

.rimage-wrapper.lazyload--placeholder {
  background: #f4f4f4;
}

.rimage-wrapper .rimage__image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.rimage-wrapper .rimage__image.blur-in {
  -webkit-filter: blur(10px);
  filter: blur(10px);
  transition: filter 400ms, -webkit-filter 400ms;
}

