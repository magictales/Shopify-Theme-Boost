}

.search-form__button {
  display: none;
}

.search-bar__results {
  display: none;
  position: absolute;
  left: -30px;
  top: 100%;
  width: calc(100% + 60px);
  color: #212B36;
  background: #fff;
  padding: 15px 0;
  box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.15);
