}

.mobile-menu-search-bar .search-bar__results {
  background-color: {{ mobile_menu_bck_color }};
  color: {{ color_mobile_menu_text }};
}

.hide-for-search {
  z-index: 1;
}

.search-bar-open .hide-for-search {
  opacity: 0;
  pointer-events: none;
}

.mobile-search,
.mobile-menu-search-bar {
  display: none;
}

.mobile-menu-search-bar {
  margin: 0;
  padding: 0 20px;
  width: 100%;
}

.mobile-menu-search {
  padding-bottom: 5px;
  background: rgba(0, 0, 0, 0.1);
  border-radius: 3px;
  opacity: 1;
  width: auto;
